<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0">

    <operations>
        <operation id="deleteConfigValue">
            <invocation>
                <rest method="DELETE">${config.service.url}/applications/{code}/config/{configCode}</rest>
            </invocation>
            <in>
                <field id="code" mapping="['code']"/>
                <field id="configCode" mapping="['configCode']"/>
            </in>
        </operation>
    </operations>
</object>