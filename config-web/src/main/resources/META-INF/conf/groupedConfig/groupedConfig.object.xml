<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0">
    <operations>
        <operation id="save">
            <invocation>
                <rest method="PUT">${config.service.url}/applications/{appCode}/configs</rest>
            </invocation>
            <in>
                <field id="appCode" mapping="['appCode']"/>
                <field id="data" mapping="['data']"/>
            </in>
        </operation>
    </operations>
</object>
