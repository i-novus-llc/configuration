<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">

    <list result-mapping="#this">
        <rest filters-separator="&amp;">${config.service.url}/byAppCode/lkb-rdm-frontend</rest>
    </list>
    <unique filters="id">
        <rest>${security.admin.url}/applications/{id}</rest>
    </unique>

    <fields>
        <field id="id" domain="integer">
            <select/>
        </field>

        <field id="name" domain="string">
            <select/>
        </field>

        <field id="description" domain="string">
            <select/>
        </field>

        <field id="priority" domain="string">
            <select/>
        </field>

        <field id="configs" domain="object[]">
            <select/>
        </field>

    </fields>

</query>